
@{
    ViewData["Title"] = "AddProject";
    Layout = "~/Views/Shared/_LayoutSuperAdmin.cshtml";
}

<h2>Add Project</h2>

<div>
    <table border="1" class="table table-striped table-bordered bootstrap-datatable datatable" style="width:400px">
        <tr>
            <th>Add Project Details</th>
        </tr>
        <tr>

            <td class="center">
                <div class="form-group has-success">
                    <label class="control-label" for="inputSuccess1">Project Code</label>
                    <input type="text" class="form-control" id="txtProjectCode">
                </div>
            </td>
           
        </tr>
        <tr>

            <td class="center">
                <div class="form-group has-success">
                    <label class="control-label" for="inputSuccess1">Project Name</label>
                    <input type="text" class="form-control" id="txtProjectName">
                </div>
            </td>
        </tr>
        <tr>

            <td class="center">
                <div class="form-group has-success">
                    <label class="control-label" for="inputSuccess1">Naturday Of Industry</label>
                    <input type="text" class="form-control" id="txtNature">
                </div>
            </td>
        </tr>

        <tr>
            <td class="center">

                <a class="btn btn-success" href="#" onclick="fn()">
                    <i class="glyphicon glyphicon-zoom-in icon-white"></i>
                    Create User
                </a>
                <a class="btn btn-danger" href="#">
                    <i class="glyphicon glyphicon-trash icon-white"></i>
                    Clear
                </a>
                <a class="btn btn-info" href="#">
                    <i class="glyphicon glyphicon-edit icon-white"></i>
                    All Users
                </a>
            </td>
            
        </tr>
        
    </table>
</div>
